

<div class="panel panel-default">
    <div class="panel-heading">Meshes list</div>
    <div class="panel-body">
        <div class="list-group">
            <button type="button" class="list-group-item" ng-repeat="n in ui.MeshList track by $index">{{n}}</button>
        </div>
        <div class="btn-group pull-right">
            <button class="btn btn-success" ng-click="ui.showMeshUploadPanel = true;">Add mesh</button>
            <button class="btn btn-danger">DeleteMesh</button>
        </div>
    </div>
</div>
